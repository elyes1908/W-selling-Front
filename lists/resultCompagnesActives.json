            "primaryKey": "Itemkey",
            "sql": "SELECT total_jobs.id_firm, total_jobs.jobs as jobs, total_companies.entreprises, compagne.c_nom\r\nfrom (SELECT association_jobs.id_firm, count(association_jobs.id) as jobs\r\n      FROM app_fd_jobs jobs\r\n               JOIN association_firms_jobs association_jobs ON jobs.id = association_jobs.id_job\r\n\r\n      group by id_firm\r\n     ) total_jobs\r\n         left join\r\n     (SELECT association_company.id_firm, count(association_company.id) as entreprises\r\n      FROM app_fd_company_overview company\r\n               JOIN association association_company ON company.id = association_company.id_company\r\n      group by id_firm\r\n     ) total_companies on total_jobs.id_firm = total_companies.id_firm\r\n         JOIN app_fd_compagnes compagne ON compagne.id = total_jobs.id_firm\r\nwhere compagne.c_statut like \"active\""
        }
    },
