 if(con != null) {\r\n            con.close();\r\n        }\r\n    } catch(SQLException e) {\r\n    }\r\n}\r\n}\r\n\r\n\r\ntry {\r\n    update();\r\n    \r\n} catch (ParseException e) {\r\n    e.printStackTrace();\r\n}"}
